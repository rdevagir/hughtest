                    var schedule = '{!schedule}';

                    $schedule.val(schedule).trigger('change');

                    if(schedule == 'Custom') {
                        $frequencyType.val('{!frequencyType}');
                        $frequencyValue.val('{!frequencyValue}');
                    }

                    var sObjectDateFieldsListValue = '{!sObjectDateField}' || '{!sObjectDateFieldDefault}';
