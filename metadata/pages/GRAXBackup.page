                    if(scheduledProcessType == 'Multi-Object') {
                        $timeSelect.removeClass('slds-hide');
                        renderObjectsSelection();
                    }

                    $scheduledProcessModal.toggleClass('isReadOnly', {!isReadOnly});
                };
            </script>
